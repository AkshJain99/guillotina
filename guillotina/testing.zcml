<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:plone="http://namespaces.plone.org/plone"
           i18n_domain="guillotina">

  <include package="guillotina" file="meta.zcml" />

  <plone:contenttype
      portal_type="File"
      schema=".content.IItem"
      class=".content.Item"
      behaviors=".behaviors.dublincore.IDublinCore
                 .behaviors.attachment.IAttachment"
    />

  <plone:contenttype
      portal_type="Example"
      schema=".testing.IExample"
      class=".testing.Example"
      behaviors=".behaviors.dublincore.IDublinCore
                 .behaviors.attachment.IAttachment"
    />

</configure>
